EXECUTABLE  flow
STDERR      flow.stderr
STDOUT      flow.stdout

DEFAULT  <OPTS1>  "--enable-esmry=true"
DEFAULT  <OPTS2>  "--solver-max-time-step-in-days=31.0"
DEFAULT  <OPTS3>  "--local-well-solve-control-switching=true"
DEFAULT  <OPTS4>  "--use-implicit-ipr=true"
DEFAULT  <OPTS5>  "--threads-per-process=1"

ARGLIST <ECLBASE> <OPTS1> <OPTS2> <OPTS3> <OPTS4> <OPTS5>
